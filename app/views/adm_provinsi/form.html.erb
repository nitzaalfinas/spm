<form class="pt10 pr10 pb10 pl10" id="provinsi_form">
	<table>
		<tr>
			<td>
				ID
			</td>
			<td>
				<input type="text" name="id" value="<%= @provinsi.id %>" />
			</td>
		</tr>

		<tr>
			<td>
				Nama Provinsi
			</td>
			<td>
				<input type="text" name="provinsi_nama" value="<%= @provinsi.provinsi_nama %>" />
			</td>
		</tr>

		<tr>
			<td></td>
			<td align="right">
				<input type="submit" class="button tiny" value="Simpan" onclick="admProvinsi.formSave(<%= @provinsi.id %>);return false;">
			</td>
		</tr>
	</table>
</form>
