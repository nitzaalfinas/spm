<form class="pt10 pr10 pb10 pl10" id="pengguna_form">
	<table>
		<tr>
			<td>
				ID
			</td>
			<td>
				<input type="text" name="id" value="<%= @user.id %>" />
			</td>
		</tr>

		<tr>
			<td>
				Provinsi
			</td>
			<td>
				<input type="text" name="provinsi_nama" value="<%= @user.provinsi_nama %>" disabled readonly />
			</td>
		</tr>

		<tr>
			<td>
				Kabupaten/Kota
			</td>
			<td>
				<input type="text" name="kabkot_nama" value="<%= @user.kabkot_nama %>" disabled readonly />
			</td>
		</tr>

		<tr>
			<td>
				Level
			</td>
			<td>
				<input type="text" name="level" value="<%= @user.level %>" />
			</td>
		</tr>

		<tr>
			<td></td>
			<td align="right">
				<input type="submit" class="button tiny" value="Simpan" onclick="admPengguna.formSaveEdit(<%= @user.id %>);return false;">
			</td>
		</tr>
	</table>
</form>
